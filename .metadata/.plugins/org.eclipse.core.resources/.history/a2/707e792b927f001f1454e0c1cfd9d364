package sample02;

import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Component;

@Component("calc2")
@Scope("prototype")
public class CalcMul implements Calc {
	@Override
	public void calc(int x, int y) {
		System.out.println(x + " * " + y + " = " + (x*y));
		
	}
}
