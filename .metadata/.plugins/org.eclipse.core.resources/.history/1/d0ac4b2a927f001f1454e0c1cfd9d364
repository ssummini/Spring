package sample02;

import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Component;

@Component("calc")
@Scope("prototype")
public class CalcAdd implements Calc {
	@Override
	public void calc(int x, int y) {
		
		System.out.println(x + " + " + y + " = " + (x+y));
		
	}
}
