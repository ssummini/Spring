package sample01;

//공통관심사항
public class LoggingAdvice {
	
	public void beforeTrace() {
		System.out.println("before trace");
	}
	
	public void afterTrace() {
		System.out.println("after trace");
	}
	
	public void Trace() {
		System.out.println("입실");
		
		핵심코드를 수행하는 메소드를 호출한다.
		
		System.out.println("퇴실");
	}
}
